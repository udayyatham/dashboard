<script type="text/javascript" >
	$(document).ready(function() {
	    $(".planInfo").click(function(event) {	        
	        $.ajax({
		    type:'GET',
		    url: $(this).attr('href'),
		    success: function(data){
		    	$("#suiteInfo").hide();
		        $("#planDetails").html($(data).filter('.displaycobrandwise'));
		    } 
});
	        event.preventDefault();
	    });
});â€‹
